{
  "name": "mcp-terminal-server",
  "version": "1.0.0",
  "description": "Secure MCP TypeScript SDK server for safe terminal command execution",
  "main": "dist/server.js",
  "bin": {
    "mcp-terminal-server": "dist/server.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx src/server.ts",
    "test": "jest",
    "test:security": "npm audit && npm test",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "terminal",
    "command-execution",
    "security",
    "claude",
    "typescript"
  ],
  "author": "MCP Terminal Server Contributors",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "zod": "^3.22.4",
    "validator": "^13.11.0",
    "winston": "^3.11.0",
    "rate-limiter-flexible": "^5.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/validator": "^13.11.7",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "prettier": "^3.1.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/luiscnext/mcp-terminal-server.git"
  },
  "bugs": {
    "url": "https://github.com/luiscnext/mcp-terminal-server/issues"
  },
  "homepage": "https://github.com/luiscnext/mcp-terminal-server#readme"
}
